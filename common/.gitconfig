[user]
    name = DF_XYZ
    email = dfxyz1@gmail.com

[alias]
    br = branch
    co = checkout
    cm = commit
    amd = commit --amend
    lg = log --graph --format='%C(blue)%h %C(green)%ci %C(yellow)%an%C(red)%d\n%s\n%b\n'
    lga = lg --all
    mg = merge --no-ff
    pf = push -f
    st = status

[init]
    defaultBranch = master

[core]
    autocrlf = input
    quotePath = false

[push]
    default = simple
